<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleDotIndicator">
        <attr name="image_height" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="image_width" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="image_src" format="reference|color" />
        <attr name="circle_dot_count" format="integer" />
    </declare-styleable>
</resources>
